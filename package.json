{
  "name": "fractals-tests",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:validate": "vitest --run",
    "lint": "eslint ./src --report-unused-disable-directives --max-warnings 0",
    "validate": "yarn lint && yarn typecheck && yarn test:validate",
    "preview": "vite preview",
    "format": "prettier --write ./src",
    "format:validate": "prettier --check ./src"
  },
  "dependencies": {
    "@mantine/core": "^8.2.8",
    "@mantine/form": "^8.2.8",
    "@mantine/hooks": "^8.2.5",
    "@tanstack/react-router": "^1.139.14",
    "@tanstack/react-router-devtools": "^1.139.14",
    "@types/node": "^24.7.0",
    "immer": "^10.1.3",
    "react": "^19",
    "react-dom": "^19",
    "react-icons": "^5.5.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@tanstack/router-plugin": "^1.139.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.14.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.7.4",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.22.0",
    "vite": "^5.3.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.3.1"
}
